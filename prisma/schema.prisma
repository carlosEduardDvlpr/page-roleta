generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model results_bets {
  id            Int      @id @default(autoincrement())
  result_number Int
  date          DateTime @db.Timestamp(6)
}
